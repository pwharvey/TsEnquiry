  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"qStockPositionAvailable"."BranchID" "BranchID"
	,"qStockPositionAvailable"."ProductCode" "ProductCode"
	,"qStockPositionAvailable"."LocationCode" "LocationCode"
	,"qStockPositionAvailable"."PacketNo" "PacketNo"

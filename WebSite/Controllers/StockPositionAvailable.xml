	,"qStockPositionAvailable"."Width" "Width"
	,"qStockPositionAvailable"."Thickness" "Thickness"
	,"qStockPositionAvailable"."Length" "Length"
	,"qStockPositionAvailable"."Cube" "Cube"
	,"qStockPositionAvailable"."Lineal" "Lineal"
	,"qStockPositionAvailable"."Pieces" "Pieces"
	,"qStockPositionAvailable"."LastMovementDate" "LastMovementDate"
	,"qStockPositionAvailable"."LastProcessCode" "LastProcessCode"
	,"qStockPositionAvailable"."CreatedDate" "CreatedDate"
	,"qStockPositionAvailable"."CreatedProcessCode" "CreatedProcessCode"
	,"qStockPositionAvailable"."SupplierName" "SupplierName"
	,"qStockPositionAvailable"."PieceTally" "PieceTally"
	,"qStockPositionAvailable"."CreatedRunNo" "CreatedRunNo"
	,"qStockPositionAvailable"."AvailableStatus" "AvailableStatus"
	,"qStockPositionAvailable"."LastRunNo" "LastRunNo"
	,"qStockPositionAvailable"."KilnDriedDate" "KilnDriedDate"
from "wa"."qStockPositionAvailable" "qStockPositionAvailable"
]]></text>
    </command>
